<% include partials/header %>
    <div class="profile-container">
        <p class="label">Upload</p>
        <div class="profile-container-content">
            <div class="profile-container-content-heading">
                <p> Post Title</p>
                <p>Url</p>
            </div>
            <div class="profile-container-content-details">
                <form action="/home/update/<%= results[0].uploadid %>" method="POST">
                    <input type="text" class="profile-input" name="title" placeholder="Title" required value="<%= results[0].title %>">
                    <input type="text" class="profile-input" name="url" placeholder="Url" required value="<%= results[0].url %>">
                    <br>
                    <button class="button"><span>Post </span></button>
                </form>
            </div>
        </div>
    </div>
    <script>
        var select = document.getElementById('2')
        select.onload = load();
        function load() {
            select.classList.add('side-nav-item-active');
        }
    </script>  
<% include partials/footer %>

